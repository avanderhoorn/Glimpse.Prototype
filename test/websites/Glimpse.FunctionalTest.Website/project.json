{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "dependencies": {
    "Glimpse.Agent.AspNet.Mvc": "2.0.0-*",
    "Glimpse.Server": "2.0.0-*",
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
